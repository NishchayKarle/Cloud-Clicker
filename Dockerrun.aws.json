{
    "AWSEBDockerrunVersion": "1",
    "Image": {
        "Name": "cloud-clicker-app"
    },
    "Ports": [
        {
            "HostPort": 8080,
            "ContainerPort": 80
        }
    ],
    "Volumes": [
        {
            "HostDirectory": "/CloudClicker/logs",
            "ContainerDirectory": "/app/logs"
        }, 
        {
            "HostDirectory": "/CloudClicker/db",
            "ContainerDirectory": "/app/db"
        }
    ]
}